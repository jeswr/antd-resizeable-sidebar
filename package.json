{
  "name": "antd-resizeable-sider",
  "version": "1.0.0",
  "description": "A resizeable sider for use with the antd ui library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/sidebar.js",
    "dist/sidebar.d.ts"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint",
    "test": "jest --coverage",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeswr/antd-resizeable-sidebar.git"
  },
  "keywords": [
    "antd",
    "ui",
    "resizeable",
    "resize",
    "sider",
    "sidebar",
    "react-component"
  ],
  "author": "Jesse Wright <jesse.wright@anu.edu.au>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeswr/antd-resizeable-sidebar/issues"
  },
  "homepage": "https://github.com/jeswr/antd-resizeable-sidebar#readme",
  "devDependencies": {
    "@babel/preset-react": "7.26.3",
    "@jest/globals": "29.7.0",
    "@testing-library/react": "16.2.0",
    "@types/antd": "1.0.4",
    "@types/jest": "29.5.14",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "@typescript-eslint/eslint-plugin": "8.24.0",
    "@typescript-eslint/parser": "8.24.0",
    "eslint": "9.20.1",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-standard": "17.1.0",
    "eslint-import-resolver-typescript": "3.7.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.1.0",
    "jest": "29.7.0",
    "pre-commit": "1.2.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "ts-jest": "29.2.5",
    "typescript": "5.7.3"
  },
  "dependencies": {
    "antd": "^5.23.3",
    "react": "^19.0.0"
  },
  "peerDependencies": {
    "antd": "^5.23.3",
    "react": "^19.0.0",
    "react-dom": "^17.0.1"
  },
  "pre-commit": [
    "build",
    "lint",
    "test"
  ],
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "**/__tests__/**/*-test.[jt]s?(x)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/*.tsx"
    ]
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
