{
  "name": "antd-resizeable-sider",
  "version": "1.0.0",
  "description": "A resizeable sider for use with the antd ui library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/sidebar.js",
    "dist/sidebar.d.ts"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint",
    "test": "jest --coverage",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeswr/antd-resizeable-sidebar.git"
  },
  "keywords": [
    "antd",
    "ui",
    "resizeable",
    "resize",
    "sider",
    "sidebar",
    "react-component"
  ],
  "author": "Jesse Wright <jesse.wright@anu.edu.au>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeswr/antd-resizeable-sidebar/issues"
  },
  "homepage": "https://github.com/jeswr/antd-resizeable-sidebar#readme",
  "devDependencies": {
    "@babel/preset-react": "7.18.6",
    "@jest/globals": "28.1.3",
    "@testing-library/react": "12.1.5",
    "@types/antd": "1.0.0",
    "@types/jest": "27.5.0",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.3",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-standard": "16.0.3",
    "eslint-import-resolver-typescript": "3.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.2.0",
    "eslint-plugin-react": "7.31.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "26.6.3",
    "pre-commit": "1.2.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "ts-jest": "26.5.6",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "antd": "^4.9.3",
    "react": "^17.0.1"
  },
  "peerDependencies": {
    "antd": "^4.9.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "pre-commit": [
    "build",
    "lint",
    "test"
  ],
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "**/__tests__/**/*-test.[jt]s?(x)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/*.tsx"
    ]
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
